{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Messaging": {
    "Kafka": {
      "EnableConsoleLogging": false,
      "IsEnabled": true,
      "ConsumerConfig": {
        "BootstrapServers": "localhost:9092",
        "GroupId": "Samples.Messaging.Kafka",
        "AutoOffsetReset": 1,
        "EnableAutoCommit": true,
        "AutoCommitIntervalMs":  0
      },
      "ProducerConfig": {
        "BootstrapServers": "localhost:9092",
        "ClientId": "192.168.1.34"
      }
    }
  }
}
